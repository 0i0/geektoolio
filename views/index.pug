html
  head
    title geektoolio
    link(rel="stylesheet" href="css/reset.css")
    link(rel="stylesheet" href="css/style.css")
    script(src="js/lib/ajax.js")
    script(src="js/lib/maketable.js")
    script(src="js/lib/ring.js")
    script(src="js/lib/chart.min.js")
  body

    div#cpu.title.h1 CPU
    div#cpu-data.stats
    div#cputemp-data.stats 0
    div#mem.title.h1 Mem
    div#mem-data.stats
      span#memusage-data 0 
      span of 
      span#memtotal-data 0
    table#net.stats(style="width:190px")
      tr
        td(style="width:100px")
        td.h1(style="width:40px") Up 
        td#netup-data.net-data(style="width:50px")
      tr
        td(style="width:100px")
        td.h1(style="width:40px") Down 
        td#netdn-data.net-data(style="width:50px")
        
    table#ps-data.stats
      thead
        tr
          td.h1(style="width:80px") Proccess
          td.h1(style="width:45px") PID
          td.h1(style="width:45px") Cpu
          td.h1(style="width:45px") Mem
      tbody#ps-tbody
    table#crypto-data.stats
      thead
        tr
          td.h1(style="width:80px")
          td.h1(style="width:45px")
      tbody#crypto-body
    canvas#myCanvas(width=200 height=130)
    canvas#up-chart.stats(width=100 height=35)
    canvas#dn-chart.stats(width=100 height=35)
    script(src="js/script.js")
